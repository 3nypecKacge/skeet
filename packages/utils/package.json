{
  "name": "@skeet-framework/utils",
  "version": "1.3.2",
  "author": "ELSOUL LABO B.V.",
  "license": "Apache-2.0",
  "private": false,
  "description": "Skeet Framework Plugin - Utils",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/elsoul/skeet.git"
  },
  "engines": {
    "pnpm": ">=8",
    "npm": "please_use_pnpm_instead",
    "node": ">=20"
  },
  "keywords": [
    "TypeScript",
    "Serverless",
    "Utils",
    "Skeet Plugin"
  ],
  "bugs": {
    "url": "https://discord.gg/ausnBvAM38"
  },
  "homepage": "https://skeet.dev",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "vitest",
    "build": "rm -rf dist/ && npx ts-node -r tsconfig-paths/register --transpile-only build.ts && tsc --emitDeclarationOnly && mv -f dist/src/* dist/ && rm -rf dist/src/",
    "build-doc": "typedoc src/index.ts --out docs",
    "dev": "npx ts-node -r tsconfig-paths/register --transpile-only src/index.ts",
    "start": "node dist/index.js",
    "discord:labo": "npx ts-node -r tsconfig-paths/register --transpile-only src/lib/runDiscordChangeLog.ts labo",
    "discord:epics": "npx ts-node -r tsconfig-paths/register --transpile-only src/lib/runDiscordChangeLog.ts epics",
    "version": "npx ts-node -r tsconfig-paths/register --transpile-only src/lib/logVersion.ts"
  },
  "dependencies": {
    "date-fns": "3.3.1",
    "date-fns-tz": "1.2.2",
    "dotenv": "16.4.5",
    "node-fetch": "2.6.9"
  },
  "devDependencies": {
    "vite": "5.1.4",
    "@types/node": "20.11.19",
    "@types/node-fetch": "2.6.11",
    "babel-loader": "9.1.3",
    "esbuild": "0.20.1",
    "esbuild-plugin-alias-path": "2.0.2",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "prettier": "3.2.5",
    "ts-loader": "9.5.1",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typedoc": "0.25.8",
    "typescript": "5.3.3",
    "vite-tsconfig-paths": "4.3.1",
    "vitest": "1.3.1"
  }
}
