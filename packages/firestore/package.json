{
  "name": "@skeet-framework/firestore",
  "version": "2.5.0",
  "description": "Skeet Framework Plugin - Firestore",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/elsoul/skeet.git"
  },
  "keywords": [
    "TypeScript",
    "Serverless",
    "Web3js",
    "Skeet Plugin",
    "Solana"
  ],
  "author": "ELSOUL LABO B.V.",
  "license": "Apache-2.0",
  "private": false,
  "bugs": {
    "url": "https://github.com/elsoul/skeet/issues"
  },
  "homepage": "https://skeet.dev",
  "scripts": {
    "test": "vitest",
    "build": "rm -rf dist/ && tsx build.ts && tsc --emitDeclarationOnly && mv -f dist/src/* dist/ && rm -rf dist/src/",
    "test:ci": "tsx firebase emulators:exec --only firestore 'yarn run test'",
    "dev:emulator": "firebase emulators:start --only firestore --project fakeproject",
    "build-doc": "typedoc src/index.ts --out docs",
    "dev": "tsx src/index.ts",
    "lint": "eslint --ext .ts,.js --fix ."
  },
  "dependencies": {
    "dotenv": "16.4.5",
    "firebase": "10.8.1",
    "firebase-admin": "12.0.0"
  },
  "devDependencies": {
    "@firebase/rules-unit-testing": "3.0.1",
    "@types/jest": "29.5.12",
    "@types/node": "20.11.24",
    "@typescript-eslint/eslint-plugin": "7.1.0",
    "@typescript-eslint/parser": "7.1.0",
    "esbuild": "0.20.1",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "husky": "9.0.11",
    "lint-staged": "15.2.2",
    "prettier": "3.2.5",
    "ts-loader": "9.5.1",
    "tsconfig-paths": "4.2.0",
    "tsx": "4.7.1",
    "typedoc": "0.25.10",
    "typescript": "5.3.3",
    "vite-tsconfig-paths": "4.3.1",
    "vitest": "1.3.1"
  },
  "lint-staged": {
    "*.{ts,css,md}": "prettier --write",
    "{src,tests}/**/*.ts": "eslint --fix"
  }
}
