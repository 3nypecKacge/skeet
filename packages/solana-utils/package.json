{
  "name": "@skeet-framework/solana-utils",
  "version": "0.2.10",
  "description": "Skeet Framework Plugin - Solana Utils",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/src/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/elsoul/skeet.git"
  },
  "keywords": [
    "TypeScript",
    "Serverless",
    "Solana",
    "Web3js",
    "Web3",
    "Blockchain",
    "Skeet Plugin",
    "framework",
    "Validator Tools"
  ],
  "author": "ELSOUL LABO B.V.",
  "license": "Apache-2.0",
  "private": false,
  "bugs": {
    "url": "https://github.com/elsoul/skeet/issues"
  },
  "homepage": "https://skeet.dev",
  "scripts": {
    "test": "vitest",
    "build": "rm -rf dist/ && npx ts-node -r tsconfig-paths/register --transpile-only build.ts && tsc --emitDeclarationOnly && mv -f dist/src/* dist/ && rm -rf dist/src/",
    "build-doc": "typedoc src/index.ts --out docs",
    "publish": "npm publish",
    "dev": "npx ts-node -r tsconfig-paths/register --transpile-only src/index.ts",
    "discord:labo": "npx ts-node -r tsconfig-paths/register --transpile-only ./.github/workflows/runDiscordChangeLog.ts labo",
    "discord:epics": "npx ts-node -r tsconfig-paths/register --transpile-only ./.github/workflows/runDiscordChangeLog.ts epics"
  },
  "dependencies": {
    "@metaplex-foundation/mpl-token-metadata": "3.2.0",
    "@metaplex-foundation/umi": "0.9.1",
    "@metaplex-foundation/umi-bundle-defaults": "0.9.1",
    "@metaplex-foundation/umi-web3js-adapters": "0.9.1",
    "@solana/spl-token": "0.4.1",
    "@solana/web3.js": "1.90.0",
    "node-fetch": "3.3.2"
  },
  "devDependencies": {
    "@skeet-framework/discord-utils": "workspace:*",
    "@skeet-framework/utils": "workspace:*",
    "@types/node": "20.11.20",
    "@types/node-fetch": "2.6.11",
    "babel-loader": "9.1.3",
    "esbuild": "0.20.1",
    "esbuild-plugin-alias-path": "2.0.2",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "prettier": "3.2.5",
    "ts-jest": "29.1.2",
    "ts-loader": "9.5.1",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typedoc": "0.25.9",
    "typescript": "5.3.3",
    "vite-tsconfig-paths": "4.3.1",
    "vitest": "1.3.1"
  }
}
